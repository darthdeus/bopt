#!/usr/bin/env python

import sys
import os
import inspect
import tensorflow as tf
tf.enable_eager_execution()

package_dir = os.path.abspath(
    os.path.join(
        os.path.dirname(os.path.abspath(inspect.stack()[0][1])),
        "..",
    )
)

# TODO: temporary hack
sys.path.append(str(package_dir))

import bopt.cli.cli as cli

cli.main()
